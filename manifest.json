{
    "name": "content warning",
    "version": "1.2",
    "description": "HackNC2019",
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "interface.html"
      },
    "icons": { "16": "cwicon16.png",
      "48": "cwicon48.png",
      "128": "cwicon128.png"},
    "background": {
        "scripts": ["script.js"],
        "persistent": false
      },
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["script.js"],
      "matches": ["https://www.facebook.com/*"]
    }],
    "manifest_version": 2
  }